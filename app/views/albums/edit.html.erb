<div class='row justify-content-center'>
	<h1>Editing Album</h1>
</div>
<div class='row justify-content-center'>
	<%= form_for @album, url: user_album_path(@album) do |form| %>
	<%= hidden_field_tag :authenticity_token, form_authenticity_token -%>
	<%= render 'form',album:@album, locals: {form: form}%>
	<% end %>
</div>
<div class='row justify-content-center'>
	<%= link_to 'Show', user_album_path(@album) %> |
	<%= link_to 'Back', user_albums_path %>
</div>